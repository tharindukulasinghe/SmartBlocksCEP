pragma solidity >=0.4.22 <0.7.0;

contract {{name}} {

    struct Event {
        {{#attributes}}
        {{type}} {{name}}
        {{/attributes}}
    }

    Event[] public processedEvents;

    function process(Event incomingEvent){

            if({{{expression}}}){
                processedEvents.push(Event({
                    {{#outAttributes}}
                    {{rename}}: incomingEvent.{{name}}
                    {{/outAttributes}}
                }));
            }
    }
}